{
  "author": {
    "name": "jopemachine",
    "email": "jopemachine@naver.com",
    "url": "github.com/jopemachine/alfred-evernote-workflow"
  },
  "bugs": {
    "url": "https://github.com/jopemachine/alfred-evernote-workflow/issues"
  },
  "bundleDependencies": false,
  "dependencies": {
    "@jxa/global-type": "^1.3.1",
    "@jxa/run": "^1.3.2",
    "alfy": "git+https://github.com/jopemachine/alfy",
    "clipboardy": "^2.3.0",
    "evernote": "^2.0.5",
    "lodash": "^4.17.15",
    "os-locale": "^5.0.0"
  },
  "deprecated": false,
  "description": "Evernote workflow for Alfred 4",
  "devDependencies": {
    "alfy-test": "git+https://github.com/jopemachine/alfy-test",
    "ava": "^3.12.1",
    "esm": "^3.2.25",
    "husky": "^4.3.0",
    "xo": "^0.17.1"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "info.plist",
    "*.js",
    "*.scpt",
    "icon.png",
    "imgs/*.png",
    "icon/*.png",
    "initialCachingDescription.html",
    "searchConfig.json",
    "authConfigTemplate.json",
    "commandLogTemplate.json",
    "search_content/htmlCacheLogTemplate.json",
    "search_content/thumbNailPathTemplate.json",
    "*.md",
    "license"
  ],
  "homepage": "https://github.com/jopemachine/alfred-evernote-workflow#readme",
  "keywords": [
    "alfred",
    "alfy",
    "evernote",
    "workflow"
  ],
  "license": "MIT",
  "name": "alfred-evernote-workflow",
  "repository": {
    "url": "git://github.com/jopemachine/alfred-evernote-workflow.git"
  },
  "scripts": {
    "postinstall": "alfy-init && npm run initSettingFile && node printPostInstall.js",
    "preuninstall": "alfy-cleanup",
    "test": "xo && ava --verbose",
    "saveHtmlCache": "osascript saveCache.scpt",
    "clearHtmlCache": "node deleteSearchContent.js",
    "initSettingFile": "mv ./search_content/thumbNailPathTemplate.json ./search_content/thumbNailPath.json && mv authConfigTemplate.json authConfig.json && mv ./search_content/htmlCacheLogTemplate.json ./search_content/htmlCacheLog.json && mv commandLogTemplate.json commandLog.json"
  },
  "version": "0.8.3",
  "ava": {
    "files": [
      "e2e/**/*.js"
    ],
    "require": [
      "esm"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run test",
      "pre-push": "npm run test"
    }
  }
}
