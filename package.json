  },
  "bundleDependencies": false,
  "dependencies": {
    "@jxa/global-type": "^1.3.1",
    "@jxa/run": "^1.3.2",
    "alfy": "git+https://github.com/jopemachine/alfy",
    "clipboardy": "^2.3.0",
    "evernote": "^2.0.5",
    "lodash": "^4.17.15",
    "os-locale": "^5.0.0"
  },
  "deprecated": false,
  "description": "Evernote workflow for Alfred 4",
  "devDependencies": {
    "alfy-test": "^0.3.0",
    "ava": "^0.18.2",
    "xo": "^0.17.1"
  },
  "engines": {
    "node": ">=4"
  },
  "files": [
    "*.js",
    "*.png",
    "*.scpt",
    "imgs/*.png",
    "icon/*",
    "info.plist",
    "searchConfig.json",
    "OAuthTemplate.json",
    "commandLogTemplate.json",
    "search_content/htmlCacheLogTemplate.json"
  ],
  "homepage": "https://github.com/jopemachine/alfred-evernote-workflow#readme",
  "keywords": [
    "alfred",
    "workflow",
    "alfy",
    "evernote"
  ],
  "license": "MIT",
  "name": "alfred-evernote-workflow",
  "repository": {
    "url": "git://github.com/jopemachine/alfred-evernote-workflow.git"
  },
  "scripts": {
    "postinstall": "alfy-init && npm run initSettingFile && node printPreInstall.js && npm run saveHtmlCache && node printPostInstall.js",
    "preuninstall": "alfy-cleanup",
    "test": "xo && ava",
    "saveHtmlCache": "osascript saveCache.scpt",
    "clearHtmlCache": "node deleteSearchContent.js",
    "initSettingFile": "mv authConfigTemplate.json authConfig.json && mv ./search_content/htmlCacheLogTemplate.json ./search_content/htmlCacheLog.json && mv commandLogTemplate.json commandLog.json"
  },
  "version": "0.4.9"
}
